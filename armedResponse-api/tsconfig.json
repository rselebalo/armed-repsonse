{
   "compilerOptions": {
     "allowSyntheticDefaultImports": true,
     "declaration": false,
     "emitDecoratorMetadata": true,
     "experimentalDecorators": true,
     "skipLibCheck": true,
     "noStrictGenericChecks": true,
     "lib": [
       "dom",
       "es2015"
     ],
     "module": "es2015",
     "moduleResolution": "node",
     "sourceMap": true,
     "target": "es5"
   },
   "include": [
     "src/**/*.ts", "src/entity/Client.js"
   ],
   "exclude": [
     "node_modules"
   ],
   "compileOnSave": false,
   "atom": {
     "rewriteTsconfig": false
   }
 }